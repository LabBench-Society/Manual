	year = {1997},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/\%28SICI\%291097-4598\%28199704\%2920\%3A4\%3C461\%3A\%3AAID-MUS9\%3E3.0.CO\%3B2-8},
	keywords = {motor unit number estimation, neural network, single motor unit potential},
	pages = {461--468},
	file = {Fang et al. - 1997 - Motor unit number estimation by spatial-temporal s.pdf:C\:\\Users\\krist\\Zotero\\storage\\A6QHEXZX\\Fang et al. - 1997 - Motor unit number estimation by spatial-temporal s.pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\RBKIIMGQ\\(SICI)1097-4598(199704)204461AID-MUS93.0.html:text/html},
