	journal = {Canadian Journal of Cardiology},
	author = {Iung, Bernard and Vahanian, Alec},
	month = sep,
	year = {2014},
	pmid = {24986049, 24986049},
	pages = {962--970},
	file = {Snapshot:C\:\\Users\\krist\\Zotero\\storage\\3G79P6DN\\abstract.html:text/html},
}

@article{garver_epidemiology_1995,
