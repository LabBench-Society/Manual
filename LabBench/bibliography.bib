	number = {11},
	urldate = {2021-02-04},
	journal = {PLOS ONE},
	author = {Xia, Weiwei and Fu, Han and Liu, Haiying and Meng, Fanqi and Wang, Kaifeng},
	month = nov,
	year = {2020},
	note = {Publisher: Public Library of Science},
	keywords = {Sensory perception, Nerve fibers, Pain, Functional electrical stimulation, Blood flow, Inflammation, Neuropathy, Skin temperature},
	pages = {e0242490},
	file = {Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\DEGCK5KN\\Xia et al. - 2020 - A test-retest reliability study of assessing small.pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\2NC9HACP\\article.html:text/html},
}

@article{dirks_neuromuscular_2014,
	title = {Neuromuscular electrical stimulation prevents muscle disuse atrophy during leg immobilization in humans},
	volume = {210},
	copyright = {© 2013 Scandinavian Physiological Society. Published by John Wiley \& Sons Ltd},
	issn = {1748-1716},
	url = {http://onlinelibrary.wiley.com/doi/abs/10.1111/apha.12200},
	doi = {https://doi.org/10.1111/apha.12200},
	abstract = {Aim Short periods of muscle disuse, due to illness or injury, result in substantial skeletal muscle atrophy. Recently, we have shown that a single session of neuromuscular electrical stimulation (NMES) increases muscle protein synthesis rates. The aim was to investigate the capacity for daily NMES to attenuate muscle atrophy during short-term muscle disuse. Methods Twenty-four healthy, young (23 ± 1 year) males participated in the present study. Volunteers were subjected to 5 days of one-legged knee immobilization with (NMES; n = 12) or without (CON; n = 12) supervised NMES sessions (40-min sessions, twice daily). Two days prior to and immediately after the immobilization period, CT scans and single-leg one-repetition maximum (1RM) strength tests were performed to assess quadriceps muscle cross-sectional area (CSA) and leg muscle strength respectively. Furthermore, muscle biopsies were taken to assess muscle fibre CSA, satellite cell content and mRNA and protein expression of selected genes. Results In CON, immobilization reduced quadriceps CSA by 3.5 ± 0.5\% (P {\textless} 0.0001) and muscle strength by 9 ± 2\% (P {\textless} 0.05). In contrast, no significant muscle loss was detected following immobilization in NMES although strength declined by 7 ± 3\% (P {\textless} 0.05). Muscle MAFbx and MuRF1 mRNA expression increased following immobilization in CON (P {\textless} 0.001 and P = 0.07 respectively), whereas levels either declined (P {\textless} 0.01) or did not change in NMES, respectively. Immobilization led to an increase in muscle myostatin mRNA expression in CON (P {\textless} 0.05), but remained unchanged in NMES. Conclusion During short-term disuse, NMES represents an effective interventional strategy to prevent the loss of muscle mass, but it does not allow preservation of muscle strength. NMES during disuse may be of important clinical relevance in both health and disease.},
	language = {en},
	number = {3},
	urldate = {2021-01-23},
	journal = {Acta Physiologica},
	author = {Dirks, M. L. and Wall, B. T. and Snijders, T. and Ottenbros, C. L. P. and Verdijk, L. B. and Loon, L. J. C. van},
	year = {2014},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/apha.12200},
	keywords = {neuromuscular electrical stimulation, skeletal muscle, disuse atrophy, immobilization},
	pages = {628--641},
	file = {Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\UMP6K74T\\Dirks et al. - 2014 - Neuromuscular electrical stimulation prevents musc.pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\3M2H54MY\\apha.html:text/html},
}

@article{couppe_effects_2012,
	title = {The effects of immobilization on the mechanical properties of the patellar tendon in younger and older men},
	volume = {27},
	issn = {0268-0033},
	url = {http://www.sciencedirect.com/science/article/pii/S0268003312001283},
	doi = {10.1016/j.clinbiomech.2012.06.003},
	abstract = {Background
It remains unknown if inactivity changes the mechanical properties of the human patellar tendon in younger and older healthy persons. The purpose was to examine the effects of short-term unilateral immobilization on the structural and mechanical properties of the patellar tendon in older men and younger men, in vivo.
Methods
Eight older men and eight younger men underwent 14days of unilateral immobilization. All individuals were assessed on both sides before and after the intervention. MRI was used to assess whole patellar tendon dimensions. The mechanical properties of the patellar tendon were assessed using simultaneous force and ultrasonographic measurements during isometric ramp contractions.
Findings
In older men, tendon stiffness [Pre: mean 2949 (SD 799) vs. Post: mean 2366 (SD 774) Nmm−1, P{\textless}0.01] and Young's Modulus [Pre: mean 1.2 (SD 0.3) vs. Post: mean 1.0 (SD 0.3) GPa, P{\textless}0.05] declined with immobilization on the immobilized side. On the control side, tendon stiffness [Pre: mean 3340 (SD 1209) vs. Post: mean 2230 (SD 503), P{\textless}0.01] and Young's Modulus [Pre: mean 1.5 (SD 0.4) vs. Post: mean 0.9 (SD 0.3) GPa, P{\textless}0.05] also decreased with immobilization. In younger men, tendon stiffness [Pre: 3622 (SD 1760) vs. Post: mean 2910 (SD 1528) Nmm−1, P{\textless}0.01] and Young's Modulus [Pre: mean 1.7 (SD 1.1) vs. Post: mean 1.4 (SD 0.8) GPa, P{\textless}0.05] decreased only on the immobilized side.
Interpretation
Short-term immobilization led to impaired mechanical properties of the patellar tendon on the immobilized side in both younger men and older men, which can influence the function of the muscle–tendon complex.},
	language = {en},
	number = {9},
	urldate = {2021-01-19},
	journal = {Clinical Biomechanics},
	author = {Couppé, C. and Suetta, C. and Kongsgaard, M. and Justesen, L. and Hvid, L. G. and Aagaard, P. and Kjær, M. and Magnusson, S. P.},
	month = nov,
	year = {2012},
	keywords = {Activity, Bedrest, Elderly, Immobilization, Mechanical properties, Patellar tendon},
	pages = {949--954},
	file = {Couppé et al. - 2012 - The effects of immobilization on the mechanical pr.pdf:C\:\\Users\\krist\\Zotero\\storage\\G2XQT45F\\Couppé et al. - 2012 - The effects of immobilization on the mechanical pr.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\krist\\Zotero\\storage\\YJQXIDG6\\S0268003312001283.html:text/html},
