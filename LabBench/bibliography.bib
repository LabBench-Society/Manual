	month = aug,
	year = {2006},
	keywords = {Hyperalgesia, -score, Allodynia, Hypoalgesia, Hypoesthesia, Paradoxical heat sensation, QST, Reference data, Sensory profile, Sensory profiling, Standardized instructions},
