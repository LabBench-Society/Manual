	pages = {1255--1263},
	file = {Iwabuchi m.fl. - 2020 - Brain perfusion patterns are altered in chronic kn.pdf:C\:\\Users\\krist\\Zotero\\storage\\NMX5QLV5\\Iwabuchi m.fl. - 2020 - Brain perfusion patterns are altered in chronic kn.pdf:application/pdf},
