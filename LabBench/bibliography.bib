@article{ogara_electrophysiological_1982,
	title = {Electrophysiological correlates of rapid escape reflexes in intact earthworms, {Eisenia} foetida. {I}. {Functional} development of giant nerve fibers during embryonic and postembryonic periods},
	volume = {13},
	copyright = {Copyright © 1982 John Wiley \& Sons, Inc.},
