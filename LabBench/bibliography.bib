}

@article{dahlin_disturbed_2011,
	title = {Disturbed vibrotactile sense in finger pulps in patients with {Type} 1 diabetes—correlations with glycaemic level, clinical examination and electrophysiology},
	volume = {28},
	issn = {1464-5491},
	url = {http://onlinelibrary.wiley.com/doi/abs/10.1111/j.1464-5491.2011.03328.x},
	doi = {10.1111/j.1464-5491.2011.03328.x},
	abstract = {Diabet. Med. 28, 1045–1052 (2011) Abstract Aims In a cohort of men and women with Type 1 diabetes, prospectively followed for {\textgreater} 20 years, vibrotactile sense in fingers was investigated and related to neurophysiological tests, glycaemic level and clinical score. Methods Out of 58 patients, diagnosed at the age of 15–25 years and recruited 1984–1985, 32 patients (13 women, median age 52 years, range 44–75 years; 19 men, median age 52 years, range 39–69 years; median duration 33.5 years, range 21–52 years) accepted follow-up in 2006. Vibration thresholds were measured in finger pulps of index and little fingers bilaterally at seven frequencies and related to results of touch (monofilaments), tactile discrimination (two-point discrimination test), electrophysiology (median nerve function), glycaemic level (HbA1c levels since 1984–1985) and a clinical score. Results Vibrotactile sense was reduced in finger pulps, mainly in men, compared with an age- and gender-matched healthy control group with normal HbA1c. Vibration thresholds were increased, particularly at 250 and 500 Hz, in both index and little finger pulps. Touch and tactile discrimination correlated with vibration thresholds, but not with each other or with electrophysiology. HbA1c levels (at follow-up or mean values from five follow-ups since recruitment) did not correlate with any nerve function variables. Clinical scores correlated with vibrotactile sense, particularly at higher frequencies ({\textgreater} 125 Hz), but not with total Z-scores of electrophysiology. Duration of disease did not correlate with any variables. Conclusions Examination of vibration thresholds in index and little finger pulps may be valuable to detect neuropathy, where thresholds correlate with symptoms and tests.},
	language = {en},
	number = {9},
	urldate = {2022-11-04},
	journal = {Diabetic Medicine},
	author = {Dahlin, L. B. and Granberg, V. and Rolandsson, O. and Rosén, I. and Dahlin, E. and Sundkvist, G.},
	year = {2011},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1464-5491.2011.03328.x},
	keywords = {electrophysiology, diabetes, HbA1c, Type 1, vibrotactile sense},
	pages = {1045--1052},
	file = {Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\IC7RNSJT\\Dahlin et al. - 2011 - Disturbed vibrotactile sense in finger pulps in pa.pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\XNTIDJ3P\\j.1464-5491.2011.03328.html:text/html},
}

