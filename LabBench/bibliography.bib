	file = {Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\6EYKPMPI\\Hafner et al. - 2015 - Axonal excitability in primary amyloidotic neuropa.pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\QI3EVU2R\\mus.html:text/html},
