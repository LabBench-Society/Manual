	journal = {Journal of Neuroscience},
	author = {Cummins, Theodore R. and Howe, James R. and Waxman, Stephen G.},
	month = dec,
	year = {1998},
	pmid = {9822722},
	note = {Publisher: Society for Neuroscience
Section: ARTICLE},
	keywords = {dorsal root ganglion, excitability, sodium channel, tetrodotoxin, expression, persistent current},
	pages = {9607--9619},
	file = {Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\ACTQL3AE\\Cummins et al. - 1998 - Slow Closed-State Inactivation A Novel Mechanism .pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\BYGAMEYQ\\9607.html:text/html},
