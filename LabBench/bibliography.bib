	pages = {1027--1033},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\krist\\Zotero\\storage\\9XHGPX2Z\\942593.html:text/html;Schnabel and Struijk - 2001 - Evaluation of the cable model for electrical stimu.pdf:C\:\\Users\\krist\\Zotero\\storage\\BFEGDBGG\\Schnabel and Struijk - 2001 - Evaluation of the cable model for electrical stimu.pdf:application/pdf},
