	file = {Izumi m.fl. - 2017 - Facilitated temporal summation of pain correlates .pdf:C\:\\Users\\krist\\Zotero\\storage\\K2CRI5DZ\\Izumi m.fl. - 2017 - Facilitated temporal summation of pain correlates .pdf:application/pdf},
