	pages = {829--838},
	file = {Gu et al. - 1996 - Determination of nerve conduction velocity distrib.pdf:C\:\\Users\\krist\\Zotero\\storage\\CQ7U2DLK\\Gu et al. - 1996 - Determination of nerve conduction velocity distrib.pdf:application/pdf;IEEE Xplore Abstract Record:C\:\\Users\\krist\\Zotero\\storage\\I98H3J5Z\\508545.html:text/html},
}

