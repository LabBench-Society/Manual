	journal = {Journal of Neural Engineering},
	author = {Weitz, Andrew C. and Behrend, Matthew R. and Ahuja, Ashish K. and Christopher, Punita and Wei, Jianing and Wuyyuru, Varalakshmi and Patel, Uday and Greenberg, Robert J. and Humayun, Mark S. and Chow, Robert H. and Weiland, James D.},
	month = jan,
	year = {2014},
	note = {Publisher: IOP Publishing},
	pages = {016007},
	file = {IOP Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\F7LJC8HC\\Weitz et al. - 2014 - Interphase gap as a means to reduce electrical sti.pdf:application/pdf},
}

@article{springer_effect_2014,
	title = {The {Effect} of {Electrode} {Placement} and {Interphase} {Interval} on {Force} {Production} {During} {Stimulation} of the {Dorsiflexor} {Muscles}},
	volume = {38},
	copyright = {Copyright © 2014 International Center for Artificial Organs and Transplantation and Wiley Periodicals, Inc.},
