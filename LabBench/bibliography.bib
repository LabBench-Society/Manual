	pages = {168--174},
	file = {Fang and Mortimer - 1991 - Selective activation of small motor axons by quasi.pdf:C\:\\Users\\krist\\Zotero\\storage\\R8ZLQIN4\\Fang and Mortimer - 1991 - Selective activation of small motor axons by quasi.pdf:application/pdf;IEEE Xplore Abstract Record:C\:\\Users\\krist\\Zotero\\storage\\N2KNVBCJ\\76383.html:text/html},
