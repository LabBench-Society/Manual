@article{petersen_preoperative_2016,
	title = {Preoperative pain mechanisms assessed by cuff algometry are associated with chronic postoperative pain relief after total knee replacement},
	volume = {157},
	issn = {0304-3959, 1872-6623},
	url = {https://journals.lww.com/00006396-201607000-00005},
	doi = {10.1097/j.pain.0000000000000531},
	abstract = {Chronic postoperative pain after total knee replacement (TKR) in knee osteoarthritis (KOA) implies clinical challenges. Widespread hyperalgesia, facilitated temporal summation of pain (TSP), and impaired conditioned pain modulation (CPM) have been found in painful KOA. This exploratory study investigated postoperative pain relief 12 months after TKR in 4 subgroups of patients preoperatively profiled by mechanistic quantitative sensory testing. In 103 patients with KOA, pressure pain detection threshold (PDT) and tolerance thresholds (PTT) were assessed at the lower leg using cuff algometry. Temporal summation of pain was measured as an increase in pain intensity scores during 10 repeated (2 seconds intervals) painful cuff stimuli. Conditioned pain modulation was calculated as the relative increase in PDT during painful conditioning stimulation. The grand averages of TSP and CPM were calculated and values below or above were used for subgrouping: facilitated TSP/impaired CPM (group A, N 5 16), facilitated TSP/normal CPM (group B, N 5 15), normal TSP/impaired CPM (group C, N 5 44), and normal TSP/normal CPM (group D, N 5 28). Clinical VAS pain intensity scores were collected before and 12 months after TKR surgery and the pain relief calculated. Less pain relief was found in group A (52.0\% 6 14.0\% pain relief) than in group B (81.1\% 6 3.5\%, P 5 0.023) and group C (79.6\% 6 4.4\%, P 5 0.007), but not group D (69.4\% 6 7.9\%, P 5 0.087). Low preoperative PDT was associated with a less postoperative pain relief (R 5 20.222, P 5 0.034), whereas TSP or CPM alone showed no associations with postoperative pain relief. This explorative study indicated that patients with osteoarthritis with facilitated TSP together with impaired CPM are more vulnerable to experience less pain relief after TKR.},
	language = {en},
	number = {7},
	urldate = {2022-07-06},
	journal = {Pain},
	author = {Petersen, Kristian Kjær and Graven-Nielsen, Thomas and Simonsen, Ole and Laursen, Mogens Berg and Arendt-Nielsen, Lars},
	month = jul,
	year = {2016},
	pages = {1400--1406},
	file = {Petersen m.fl. - 2016 - Preoperative pain mechanisms assessed by cuff algo.pdf:C\:\\Users\\krist\\Zotero\\storage\\T7RTESP6\\Petersen m.fl. - 2016 - Preoperative pain mechanisms assessed by cuff algo.pdf:application/pdf},
}

@article{kurien_preoperative_2018,
