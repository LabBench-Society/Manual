@article{namba_conduction_1971,
	title = {Conduction velocity in the ulnar nerve in hemiplegic patients},
	volume = {12},
	issn = {0022-510X},
	url = {http://www.sciencedirect.com/science/article/pii/0022510X71900487},
	doi = {10.1016/0022-510X(71)90048-7},
	abstract = {Conduction velocities of the motor fibers in the ulnar nerve between the elbow and wrist were measured in 25 patients with hemiplegia of 1–50 days' duration and a mean age of 67.0 years. The “fast” motor conduction velocity was within the normal range in this age group, and there was no statistical difference between the paralyzed side (50.4±1.44 m/sec) and the unaffected side (52.3±1.32 m/sec). The “average” motor conduction velocity was 46.8±1.40 m/sec in the affected side and 50.4±1.44 m/sec in the unaffected side, with a statistically significant difference (P{\textless}0.01). The “slow” motor conduction velocity was decreased (P{\textless}0.001) in the paralyzed side (14.8±1.47 m/sec) compared with the unaffected side (24.6±1.29 m/sec) or in normal individuals. Prolonged conduction velocity in the hemiplegic side compared with the unaffected side was found in 76\% of patients when the “slow” motor conduction velocity was measured, in 16\% of patients each for the “average” motor and “fast” sensory conduction velocities, and in 4\% of patients for the “fast” motor conduction velocity. Direct injury of the nerve, and change in temperature could be ruled out as the cause of prolongation of the “slow” conduction velocity of the motor nerve fibers. Although the relation of these changes to muscle atrophy in patients with upper motor neuron lesions is not known, the findings indicate that upper motor neuron lesions affect the function of slowly-conducting fibers of the lower motor neuron.},
	language = {en},
	number = {2},
	urldate = {2020-12-10},
	journal = {Journal of the Neurological Sciences},
	author = {Namba, T. and Schuman, M. H. and Grob, D.},
	month = feb,
	year = {1971},
	pages = {177--186},
	file = {Namba et al. - 1971 - Conduction velocity in the ulnar nerve in hemipleg.pdf:C\:\\Users\\krist\\Zotero\\storage\\RKVU7RLN\\Namba et al. - 1971 - Conduction velocity in the ulnar nerve in hemipleg.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\krist\\Zotero\\storage\\95WTW4N8\\0022510X71900487.html:text/html},
}

