	pages = {407--414},
	file = {GormanMortimer1983.pdf:C\:\\Users\\krist\\Zotero\\storage\\57SELMLG\\GormanMortimer1983.pdf:application/pdf;IEEE Xplore Abstract Record:C\:\\Users\\krist\\Zotero\\storage\\L4I66SPB\\4121663.html:text/html;IEEE Xplore Abstract Record:C\:\\Users\\krist\\Zotero\\storage\\565T9WBV\\4121663.html:text/html},
