	journal = {The Journal of Physiology},
	author = {Baker, Mark D. and Chandra, Sonia Y. and Ding, Yanning and Waxman, Stephen G. and Wood, John N.},
	year = {2003},
	note = {\_eprint: https://physoc.onlinelibrary.wiley.com/doi/pdf/10.1111/j.1469-7793.2003.00373.x},
	pages = {373--382},
	file = {Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\XZZ5A334\\Baker et al. - 2003 - GTP-induced tetrodotoxin-resistant Na+ current reg.pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\2P5X7DZF\\j.1469-7793.2003.00373.html:text/html},
