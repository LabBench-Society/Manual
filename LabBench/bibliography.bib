	year = {1996},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/mus.880191002},
	keywords = {peripheral nervous system, pathophysiology, electrophysiology, threshold electrotonus, diabetes, ion channel, LAB-98-001-BIN:S002},
	pages = {1268--1275},
	file = {Horn et al. - 1996 - Abnormal axonal inward rectification in diabetic n.pdf:C\:\\Users\\krist\\Zotero\\storage\\RXZMTZSF\\Horn et al. - 1996 - Abnormal axonal inward rectification in diabetic n.pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\XWTFRBDJ\\mus.html:text/html},
