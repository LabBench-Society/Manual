	file = {Phongsamart et al. - 2002 - Effect of reference electrode position on the comp.pdf:C\:\\Users\\krist\\Zotero\\storage\\HBY2X7VX\\Phongsamart et al. - 2002 - Effect of reference electrode position on the comp.pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\RIQWZVYC\\mus.html:text/html},
