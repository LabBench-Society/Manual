	pages = {1202--1210},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\krist\\Zotero\\storage\\9ESUXNEY\\867947.html:text/html;Szlavik and Bruin - 2000 - The effect of anisotropy on the potential distribu.pdf:C\:\\Users\\krist\\Zotero\\storage\\NNZT5EIL\\Szlavik and Bruin - 2000 - The effect of anisotropy on the potential distribu.pdf:application/pdf},
