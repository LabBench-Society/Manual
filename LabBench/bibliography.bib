	pages = {1--9},
	file = {Grill and Mortimer - 1997 - Inversion of the current-distance relationship by .pdf:C\:\\Users\\krist\\Zotero\\storage\\FW3YXNMM\\Grill and Mortimer - 1997 - Inversion of the current-distance relationship by .pdf:application/pdf;IEEE Xplore Abstract Record:C\:\\Users\\krist\\Zotero\\storage\\ATK54QC6\\553708.html:text/html},
