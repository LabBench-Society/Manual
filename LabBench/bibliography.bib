	year = {2001},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/mus.1094},
	keywords = {spinal cord injury, thenar muscles, motor unit, cervical, involuntary muscle contraction},
	pages = {952--962},
	file = {Snapshot:C\:\\Users\\krist\\Zotero\\storage\\HMIT4PU6\\mus.html:text/html;Zijdewind and Thomas - 2001 - Spontaneous motor unit behavior in human thenar mu.pdf:C\:\\Users\\krist\\Zotero\\storage\\679XQAAK\\Zijdewind and Thomas - 2001 - Spontaneous motor unit behavior in human thenar mu.pdf:application/pdf},
