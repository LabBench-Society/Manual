	author = {Barikroo, Ali and Carnaby, Giselle and Bolser, Donald and Rozensky, Ronald and Crary, Michael},
	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/joor.12625},
	keywords = {rehabilitation, ageing, deglutition, deglutition disorders, electric stimulation therapy, neck muscles},
	pages = {436--441},
