	pages = {161--166},
	file = {Grill and Mortimer - 1996 - The effect of stimulus pulse duration on selectivi.pdf:C\:\\Users\\krist\\Zotero\\storage\\A7D986ME\\Grill and Mortimer - 1996 - The effect of stimulus pulse duration on selectivi.pdf:application/pdf;IEEE Xplore Abstract Record:C\:\\Users\\krist\\Zotero\\storage\\9HTM7Y5T\\481985.html:text/html},
