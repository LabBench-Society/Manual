	keywords = {decomposition EMG, motor unit potential, quantitative EMG, recording position, reference values},
	pages = {1385--1389},
	file = {Snapshot:C\:\\Users\\krist\\Zotero\\storage\\62FJR735\\mus.html:text/html},
}

