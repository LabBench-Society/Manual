	note = {Conference Name: IEEE Transactions on Biomedical Engineering},
	keywords = {Biomembranes, Cats, Cities and towns, Cochlear implants, Computational modeling, Delay, Electrical stimulation, Nerve fibers, Neuromuscular stimulation, Stochastic processes},
