	pages = {985--1000},
	file = {Catley et al. - 2014 - Is Tactile Acuity Altered in People With Chronic P.pdf:C\:\\Users\\krist\\Zotero\\storage\\HEZ4CFTT\\Catley et al. - 2014 - Is Tactile Acuity Altered in People With Chronic P.pdf:application/pdf},
