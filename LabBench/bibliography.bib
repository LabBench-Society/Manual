}

@article{grill_effect_1996,
	title = {The effect of stimulus pulse duration on selectivity of neural stimulation},
	volume = {43},
	issn = {1558-2531},
	doi = {10.1109/10.481985},
	abstract = {Choice of stimulus parameters is an important consideration in the design of neural prosthetic systems. The objective of this study was to determine the effect of rectangular stimulus pulsewidth (PW) on the selectivity of peripheral nerve stimulation. Computer simulations using a cable model of a mammalian myelinated nerve fiber indicated that shorter PW's increased the difference between the threshold currents of fibers lying at different distances from an electrode. Experimental measurements of joint torque generated by peripheral nerve stimulation demonstrated that shorter PW's generated larger torques before spillover and created a larger dynamic range of currents between threshold and spillover. Thus, shorter PW's allowed more spatially selective stimulation of nerve fibers. Analysis of the response of a passive cable model to different duration stimuli indicated that PW dependent contributions of distributed sources to membrane polarization accounted for the observed differences in selectivity.},
	number = {2},
	journal = {IEEE Transactions on Biomedical Engineering},
	author = {Grill, W.M. and Mortimer, J.T.},
	month = feb,
	year = {1996},
	note = {Conference Name: IEEE Transactions on Biomedical Engineering},
	keywords = {Nerve fibers, Computer simulation, Electrodes, Optical fiber polarization, Prosthetics, Current measurement, Optical fiber cables, Space vector pulse width modulation, Threshold current, Torque measurement},
	pages = {161--166},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\krist\\Zotero\\storage\\7PYT3GXJ\\481985.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\CX9NK2UB\\Grill and Mortimer - 1996 - The effect of stimulus pulse duration on selectivi.pdf:application/pdf},
}

@article{frahm_nerve_2016,
	title = {Nerve {Fiber} {Activation} {During} {Peripheral} {Nerve} {Field} {Stimulation}: {Importance} of {Electrode} {Orientation} and {Estimation} of {Area} of {Paresthesia}},
	volume = {19},
	copyright = {© 2015 International Neuromodulation Society},
	issn = {1525-1403},
	shorttitle = {Nerve {Fiber} {Activation} {During} {Peripheral} {Nerve} {Field} {Stimulation}},
	url = {http://onlinelibrary.wiley.com/doi/abs/10.1111/ner.12371},
	doi = {https://doi.org/10.1111/ner.12371},
	abstract = {Introduction and Aim Low back pain is one of the indications for using peripheral nerve field stimulation (PNFS). However, the effect of PNFS varies between patients; several stimulation parameters have not been investigated in depth, such as orientation of the nerve fiber in relation to the electrode. While placing the electrode parallel to the nerve fiber may give lower activation thresholds, anodal blocking may occur when the propagating action potential passes an anode. Methods A finite element model was used to simulate the extracellular potential during PNFS. This was combined with an active cable model of Aβ and Aδ nerve fibers. It was investigated how the angle between the nerve fiber and electrode affected the nerve activation and whether anodal blocking could occur. Finally, the area of paresthesia was estimated and compared with any concomitant Aδ fiber activation. Results The lowest threshold was found when nerve and electrode were in parallel, and that anodal blocking did not appear to occur during PNFS. The activation of Aβ fibers was within therapeutic range ({\textless}10V) of PNFS; however, within this range, Aδ fiber activation also may occur. The combined area of activated Aβ fibers (paresthesia) was at least two times larger than Aδ fibers for similar stimulation intensities. Conclusion No evidence of anodal blocking was observed in this PNFS model. The thresholds were lowest when the nerves and electrodes were parallel; thus, it may be relevant to investigate the overall position of the target nerve fibers prior to electrode placement.},
	language = {en},
	number = {3},
	urldate = {2021-05-12},
	journal = {Neuromodulation: Technology at the Neural Interface},
	author = {Frahm, Ken Steffen and Hennings, Kristian and Vera‐Portocarrero, Louis and Wacnik, Paul W. and Mørch, Carsten Dahl},
	year = {2016},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ner.12371},
	keywords = {electrical stimulation, pain, Computational modeling, peripheral nerve field stimulation},
	pages = {311--318},
	file = {Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\6DWLBUNN\\Frahm et al. - 2016 - Nerve Fiber Activation During Peripheral Nerve Fie.pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\9K6QDNLK\\ner.html:text/html},
}

