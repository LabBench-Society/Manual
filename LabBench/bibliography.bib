@article{palsson_experimental_2018,
	title = {Experimental {Referred} {Pain} {Extends} {Toward} {Previously} {Injured} {Location}: {An} {Explorative} {Study}},
	volume = {19},
	issn = {15265900},
	shorttitle = {Experimental {Referred} {Pain} {Extends} {Toward} {Previously} {Injured} {Location}},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1526590018301846},
	doi = {10.1016/j.jpain.2018.04.018},
	abstract = {Facilitated pain mechanisms have been demonstrated in musculoskeletal pain, but it is unclear whether a recent painful injury leaves the pain system sensitized. Pain characteristics were assessed in individuals who recently recovered from ankle pain (recovered pain group; n = 25) and sex-matched control subjects (n = 25) in response to tonic pressure pain and saline-induced pain applied at the shin muscle. Pain intensity and pain referral patterns were recorded bilaterally after the painful muscle stimulus. Pressure pain thresholds were measured at the lower legs and shoulder. Cuff pressure algometry on the lower leg was used to assess pain detection threshold, pressure evoking 6-cm pain score on a 10-cm visual analog scale, pain tolerance, temporal summation of pain, and conditioned pain modulation. Compared with in control subjects, saline-induced and pressure-induced pain in the shin muscle were more frequently felt as referred pain in the previously painful ankle (P {\textless} .05), and the pain area within the previously affected ankle was larger after saline-induced pain (P {\textless} .05). In the recovered pain group, conditioned pain modulation responses and the cuff pressure needed to reach a 6-cm pain score on a 10-cm visual analog scale was higher in the previously painful leg compared with in the contralateral leg (P {\textless} .05). No group differences were found in pressure pain threshold, pain detection threshold, pain tolerance, and temporal summation of pain.},
	language = {en},
	number = {10},
	urldate = {2022-07-06},
	journal = {The Journal of Pain},
	author = {Palsson, Thorvaldur Skuli and Boudreau, Shellie A. and Krebs, Hans Jørgen and Graven-Nielsen, Thomas},
