	pages = {2439--2447},
	file = {Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\IBZ52H3V\\Cappelen-Smith et al. - 2001 - Membrane properties in chronic inflammatory demyel.pdf:application/pdf;Snapshot:C\:\\Users\\krist\\Zotero\\storage\\SX5JYXCT\\455132.html:text/html},
}

@article{smith_functional_1998,
