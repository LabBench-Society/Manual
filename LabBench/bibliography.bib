	number = {7},
	urldate = {2021-03-09},
	journal = {Clinical Neurophysiology},
	author = {Zheng, Chaojun and Zhu, Yu and Nie, Cong and Lu, Feizhou and Zhu, Dongqing and Weber, Robert and Jiang, Jianyuan},
	month = jul,
	year = {2018},
	keywords = {Threshold tracking, Cervical spondylotic amyotrophy, Motor axonal excitability, Slow K channel},
	pages = {1383--1389},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\krist\\Zotero\\storage\\8NTTENV2\\Zheng et al. - 2018 - Altered motor axonal excitability in patients with.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\krist\\Zotero\\storage\\IEUMGIST\\S1388245718308988.html:text/html},
}

@article{nodera_cerebellum_2014,
	title = {Cerebellum {Tunes} the {Excitability} of the {Motor} {System}: {Evidence} from {Peripheral} {Motor} {Axons}},
