	pmcid = {PMC2772015},
	pages = {71--78},
	file = {Submitted Version:C\:\\Users\\krist\\Zotero\\storage\\JYLR3G33\\Periyasamy et al. - 2008 - Correlation between two-point discrimination with .pdf:application/pdf},
}

