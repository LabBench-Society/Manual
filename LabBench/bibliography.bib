	journal = {Journal of Neurophysiology},
	author = {Westling, G. and Johansson, R. S. and Thomas, C. K. and Bigland-Ritchie, B.},
	month = oct,
	year = {1990},
	note = {Publisher: American Physiological Society},
	pages = {1331--1338},
	file = {Snapshot:C\:\\Users\\krist\\Zotero\\storage\\KT4PCAXT\\jn.1990.64.4.html:text/html;Westling et al. - 1990 - Measurement of contractile and electrical properti.pdf:C\:\\Users\\krist\\Zotero\\storage\\MF2MGZ75\\Westling et al. - 1990 - Measurement of contractile and electrical properti.pdf:application/pdf},
}

@article{wesselink_model_1999,
	title = {A model of the electrical behaviour of myelinated sensory nerve fibres based on human data},
