	title = {The effect of semiconditional dorsal penile nerve electrical stimulation on capacity and compliance of the bladder with deformity in spinal cord injury patients: a pilot study},
	volume = {50},
	copyright = {© 2012 Nature Publishing Group},
	issn = {1362-4393},
	shorttitle = {The effect of semiconditional dorsal penile nerve electrical stimulation on capacity and compliance of the bladder with deformity in spinal cord injury patients},
