	keywords = {Digital nerve repair, neuromas, sensory recovery},
	pages = {148--154},
	file = {SAGE PDF Full Text:C\:\\Users\\krist\\Zotero\\storage\\EQE5P8AW\\Fakin et al. - 2016 - Long-term clinical outcome after epineural coaptat.pdf:application/pdf},
