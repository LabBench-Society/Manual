	doi = {10.1016/S0074-7742(02)50079-9},
	pages = {229--255},
	file = {Arezzo and Zotova - 2002 - Electrophysiologic measures of diabetic neuropathy.pdf:C\:\\Users\\krist\\Zotero\\storage\\X7Q8UFLF\\Arezzo and Zotova - 2002 - Electrophysiologic measures of diabetic neuropathy.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\krist\\Zotero\\storage\\ATBRILLZ\\S0074774202500799.html:text/html},
}

@article{altman_analysis_1990,
