@article{magnusson_passive_nodate,
	title = {Passive properties of human skeletal muscle during stretch maneuvers},
	volume = {8},
	issn = {1600-0838},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1600-0838.1998.tb00171.x},
	doi = {10.1111/j.1600-0838.1998.tb00171.x},
	abstract = {Despite limited scientific knowledge, stretching of human skeletal muscle to improve flexibility is a widespread practice among althletes. This article reviews recent findings regarding passive properties of the hamstring muscle group during stretch based on a model that was developed which could synchronously and continuously measure passive hamstring resistance and electromyographic activity, while the velocity and angle of stretch was controlled. Resistance to stretch was defined as passive torque (Nm) offered by the hamstring muscle group during passive knee extension using an isokinetic dynamometer with a modified thigh pad. To simulate a clinical static stretch, the knee was passively extended to a pre-determined final position (0.0875 rad/s, dynamic phase) where it remained stationary for 90s (static phase). Alternatively, the knee was extended to the point of discomfort (stretch tolerance). From the torque-angle curve of the dynamic phase of the static stretch, and in the stretch tolerance protocol, passive energy and stiffness were calculated. Torque decline in the static phase was considered to represent viscoelastis stress relaxation. Using the model, studies were conducted which demonstrated that a single static stretch resulted in a 30\% viscoelastic stress relaxation. With repeated stretches muscle stiffness declined, but returned to baseline values within 1 h. Long-term stretching (3 weeks) increased joint range of motion as a result of a change in stretch tolerance rather than in the passive properties. Strength training resulted in increased muscle stiffness, which was unaffected by daily stretching. The effectiveness of different stretching tecniques was attributed to a change in strech tolerance rather than passive properties. Inflexible and older subjects have increased muscle stiffness, but a lower stretch tolerance compared to subjects with normal flexiblity and younger subjects, respectively. Although far from all questions regarding the passive properties of humans skeletal muscle have been answered in these studies, the measurement technique permited skeletal muscle.},
	language = {en},
	number = {2},
	urldate = {2018-05-25},
	journal = {Scandinavian Journal of Medicine \& Science in Sports},
	author = {Magnusson, S. P.},
	keywords = {flexibility, passive property, stiffness, stress relaxation, stretching, viscoelastic},
	pages = {65--77},
	file = {Snapshot:C\:\\Users\\krist\\Zotero\\storage\\T8RNL2MU\\j.1600-0838.1998.tb00171.html:text/html},
}

@article{kirsch_identification_1997,
	title = {Identification of time-varying stiffness dynamics of the human ankle joint during an imposed movement},
	volume = {114},
	issn = {0014-4819, 1432-1106},
	url = {https://link.springer.com/article/10.1007/PL00005625},
	doi = {10.1007/PL00005625},
	abstract = {The time-varying stiffness dynamics of the human ankle joint were identified during a large stretch imposed upon the active triceps surae muscles. Small stochastic position perturbations were superimposed upon many repetitions of the larger movement and an ensemble time-varying identification technique was then used to characterize the relationship between the small perturbation and the torque it evoked at each sample in time throughout the movement. This technique was found to provide an excellent description of the ankle stiffness dynamics throughout this movement, with the identified stiffness impulse response functions accounting for more than 80\% of the torque variance at all times. The average low-frequency stiffness values (K low) derived from the stiffness impulse responses at each sample in time are believed to reflect primarily the instantaneous elastic properties of active crossbridges. These properties, which reflect the contractile state of the muscles more directly than force or torque measurements, have not been obtained previously from an intact muscle-joint system. We found that stiffness actually increased during the later portion of the large imposed stretch, indicating the triceps surae muscles did not yield significantly, and that the post-stretch steady-state stiffness level was approximately 60\% higher than prior to the stretch. Reflex activity evoked by the large stretch did not produce a detectable change in K low, even though this activity did produce a clear twitch-like response in joint torque beginning approximately 60 ms following stretch onset. A second-order mechanical model was found to provide an adequate characterization of stiffness dynamics for steady-state periods before and well after the imposed movement, but it could not adequately describe the observed changes in stiffness dynamics during the movement itself. However, the variation of model parameters indicated that the torque evoked by the stochastic displacement was predominantly elastic in nature. The stiffness behavior during stretch observed here for the intact human ankle joint is largely consistent with previous studies performed in isolated muscle preparations.},
	language = {en},
	number = {1},
	urldate = {2018-05-25},
	journal = {Experimental Brain Research},
	author = {Kirsch, Robert F. and Kearney, Robert E.},
	month = mar,
	year = {1997},
	pages = {71--85},
	file = {Snapshot:C\:\\Users\\krist\\Zotero\\storage\\JKF762XD\\PL00005625.html:text/html},
}

@article{roberson_objective_1995,
	title = {Objective {Measures} of {Joint} {Stiffness}},
	volume = {8},
	issn = {0894-1130},
	url = {http://www.sciencedirect.com/science/article/pii/S0894113012803152},
	doi = {10.1016/S0894-1130(12)80315-2},
	abstract = {Objective measures of joint stiffness allow for the evaluation of the effectiveness of treatment modalities. Without this, the effectiveness of therapy is not quantifiable. Presently, joint stiffness can be quantified by either passive range of motion (PROM) measurement or torque range of motion (TgROM) measurement. PROM measurement does not control the force applied, nor does it require that the other joints in the kinematic chain be held fixed. Also, it demonstrates poor interrater reliability. An idealized device melding existing technologies of constant passive motion devices and computerized workstations is proposed to allow for easier measurement of TgROM angles for analysis data `for the determination of the effectiveness of treatment modalities. J HAND THER 8:163-166, 1995.},
